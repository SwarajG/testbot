{
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ]
  },
  "extends": "airbnb",
  "parser": "babel-eslint"
}